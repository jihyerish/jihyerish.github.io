{
    "name": "@bbkit/sys-ui-web",
    "version": "0.599.0",
    "description": "sys-ui on the web",
    "files": [
        "dist"
    ],
    "exports": {
        ".": "./dist/lib.js",
        "./rblpui": "./dist/rblpui/index.js",
        "./rblpui/*": "./dist/rblpui/*.js",
        "./rui": "./dist/rui/index.js",
        "./rui/*": "./dist/rui/*.js",
        "./ui": "./dist/ui/index.js",
        "./ui/*": "./dist/ui/*.js",
        "./dl": "./dist/dl/index.js"
    },
    "repository": {
        "type": "git",
        "url": "https://bbgithub.dev.bloomberg.com/rplus-ui/bundles-ui",
        "directory": "sys-ui-web"
    },
    "scripts": {
        "dev": "vite",
        "build": "vite build",
        "preview": "vite preview",
        "test": "web-test-runner test/**/*.test.{html,js} --node-resolve",
        "test:watch": "web-test-runner test/**/*.test.{html,js} --node-resolve --watch",
        "test:ci": "web-test-runner test/**/*.test.{html,js} --node-resolve --config web-test-runner.ci.config.js"
    },
    "devDependencies": {
        "@esm-bundle/chai": "^4.3.4-fix.0",
        "@rollup/plugin-json": "^4.1.0",
        "@rollup/pluginutils": "^4.2.0",
        "@web/test-runner": "^0.13.27",
        "@web/test-runner-puppeteer": "^0.10.5",
        "sass": "^1.49.9",
        "vite": "^2.8.0"
    },
    "overrides": {
        "puppeteer": "13.5.2"
    },
    "overridesComments": {
        "puppeteer": "Pinned to chromium 970485. See `README.md#sys-ui-web-ci-docker-image` and `Dockerfile`."
    },
    "engines": {
        "npm": ">=8.3.0"
    },
    "publishConfig": {
        "registry": "https://artprod.dev.bloomberg.com/artifactory/api/npm/npm-repos/"
    }
}
